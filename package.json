    "lucide-react": "^0.555.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.6",
    "three": "^0.155.0",
    "three-stdlib": "^2.36.1",
    "uuid": "^9.0.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/three": "^0.155.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "eslint": "^8.0.0",
    "fs-extra": "^11.3.2",
    "jest": "^29.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "main": "jest.config.js",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
