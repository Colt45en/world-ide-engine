{
  "name": "nexus-world-engine",
  "version": "1.0.0",
  "private": true,
  "description": "NEXUS World Engine - Unified 3D development IDE with 6 integrated tools",
  "main": "dist-electron/main.cjs",
  "homepage": "http://./",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:studio": "vite build --mode studio",
    "build:all": "npm run build && npm run build:studio",
    "build:electron": "npm run build && electron-builder",
    "electron": "npm run build && electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist build node_modules .turbo",
    "server": "tsx src/server.ts",
    "dashboard": "tsx src/index.ts",
    "dev:all": "concurrently npm run dev npm run server",
    "pipeline:demo": "tsx src/pipeline/pipeline-demo.ts",
    "viz:demo": "tsx src/viz/viz-demo.ts",
    "viz:simple": "tsx src/viz/simple-viz-demo.ts",
    "shader:demo": "tsx src/shader/shaderBundle-demo.ts",
    "motion:demo": "tsx src/math/motion-math-demo.ts",
    "nexus:ollama": "tsx nexus-ollama-integration.ts",
    "ollama:install": "node install-nexus-ollama.js",
    "unified": "vite --config vite.unified.config.ts",
    "unified:app": "npm run dev",
    "freeworld": "vite --open /free-world-engine.html",
    "build:freeworld": "vite build --config vite.freeworld.config.ts",
    "typecheck:freeworld": "tsc --noEmit -p tsconfig.freeworld.json",
    "feel:build": "cd nucleus/compiler/LMN/Resonance/feel && cmake -B build -S . && cmake --build build --config Release",
    "feel:compile": "nucleus/compiler/LMN/Resonance/feel/feelc.exe",
    "lmn:install": "cd lmn-lang && npm install && code --install-extension .",
    "install:lmn-ext": "powershell -ExecutionPolicy Bypass -File INSTALL_LMN_EXTENSION.ps1",
    "resonance:build": "emcc src/systems/compiler/ResonanceCompiler.cpp src/systems/compiler/WasmBindings.cpp -I src/systems/compiler -o public/resonance.js -s WASM=1 -s MODULARIZE=1 -s EXPORT_NAME='ResonanceModule' -s ALLOW_MEMORY_GROWTH=1 -s \"EXPORTED_RUNTIME_METHODS=['ccall','cwrap']\" --bind -O3",
    "native:ingest_learning": "node tools/import_learning_to_native.js",
    "telemetry:demo": "node tools/demo_telemetry_server.js",
    "example:todo": "npx http-server ./examples/offline-todo-app -p 8083",
    "example:todo-backend": "node examples/backend/server.js",
    "test:e2e": "npx playwright test tests/e2e --project=chromium",
    "bench:marching": "node tools/bench/marching_benchmark.mjs",
    "bench:threads": "node tools/bench/marching_threads_benchmark.mjs",
    "bench:play": "node tools/bench/marching_stream_playwright.mjs",
    "html:validate": "node scripts/validate-html.js",
    "html:standardize": "powershell -ExecutionPolicy Bypass -File scripts/standardize-html.ps1",
    "html:check": "npm run html:validate"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/ws": "^8.18.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "express-validator": "^7.0.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.555.0",
    "nexus-core-v2.1": "file:nexus-core-v2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.10.1",
    "redis": "^5.10.0",
    "rehype-highlight": "^7.0.2",
    "three": "^0.158.0",
    "three-stdlib": "^2.36.1",
    "uuid": "^9.0.0",
    "ws": "^8.18.3",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.158.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "electron": "^39.2.4",
    "electron-builder": "^24.9.1",
    "eslint": "^8.0.0",
    "fs-extra": "^11.3.2",
    "jest": "^29.5.0",
    "jsdom": "^27.2.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.3.0",
    "vite": "^7.2.6",
    "vite-plugin-electron": "^0.14.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vitest": "^4.0.14",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.nexus.world.engine",
    "productName": "NEXUS World Engine",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "type": "module",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://8a980b8f-fb10-437b-95fb-553746d95bda@github.com/Colt45en/world-ide-engine.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Colt45en/world-ide-engine/issues"
  }
}
